<?xml version="1.0" encoding="UTF-8"?>

<mule xmlns:ee="http://www.mulesoft.org/schema/mule/ee/core" xmlns="http://www.mulesoft.org/schema/mule/core"
	xmlns:doc="http://www.mulesoft.org/schema/mule/documentation"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
http://www.mulesoft.org/schema/mule/ee/core http://www.mulesoft.org/schema/mule/ee/core/current/mule-ee.xsd">
	<sub-flow name="generate-random-text-Sub-Flow" doc:id="a4762e91-94de-4621-9916-c3f896da9c0c" >
		<ee:transform doc:name="generate random text" doc:id="8e6166c0-213c-4363-97a1-ae0cb2377c60" >
			<ee:message >
				<ee:set-payload ><![CDATA[%dw 2.0
output application/json
fun generateString(n: Number) = 
do {
  var x = ('0123456789' ++ 'ABCDEFGHIJKLMNOPQRSTUVWXYZ' ++ 'abcdefghijklmnopqrstuvwxyz' ++ '!%&')
  ---
  0 to (n-1) reduce (item, acc="") -> acc ++ x[ceil(random()*sizeOf(x))]
}
---
generateString(8)]]></ee:set-payload>
			</ee:message>
			<ee:variables >
			</ee:variables>
		</ee:transform>
		<logger level="INFO" doc:name="generate text subflow" doc:id="3e467dea-2a95-40f3-8f39-3c00c975ef18" message="Random text is generated"/>
	</sub-flow>
</mule>
